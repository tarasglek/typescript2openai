{
  "name": "typescript2openai",
  "version": "1.0.1",
  "description": "A utility to convert TypeScript/JavaScript function signatures with jsdoc comments to OpenAI function spec",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "build": "unbuild"
  },
  "keywords": [
    "openai",
    "jsonschema",
    "typescript"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Taras Glek",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.5.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "acorn": "^8.10.0",
    "acorn-typescript": "^1.4.1"
  }
}
